{
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist/spa",
  "framework": "vite",
  "nodeVersion": "18.x",
  "env": {
    "OAUTH_CLIENT_ID": "@oauth_client_id",
    "OAUTH_CLIENT_SECRET": "@oauth_client_secret"
  },
  "regions": ["iad1"],
  "functions": {
    "netlify/functions/api.ts": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "redirects": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
