{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/spa" }
    }
  ],
  "rewrites": [
    { "source": "/admin/config.yml", "destination": "/admin/config.yml" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
